@font-face {
    font-family: 'gt_walsheim_bold';
    src: <%= asset_url "gt-walsheim-bold-web.eot" %>
    src: <%= asset_url "gt-walsheim-bold-web.eot" %>?#iefix format('embedded-opentype'),
         <%= asset_url "gt-walsheim-bold-web.woff" %> format('woff'),
         <%= asset_url "gt-walsheim-bold-web.ttf" %> format('truetype'),
         <%= asset_url "gt-walsheim-bold-web.svg" %>#gt_walsheim_bold format('svg');
    font-weight: normal;
    font-style: normal;
}