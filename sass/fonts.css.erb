@font-face {
    font-family: 'gt_walsheim_bold';
    src: url('<%= font_path "gt-walsheim-bold-web.eot" %>');
    src: url('<%= font_path "gt-walsheim-bold-web.eot" %>?#iefix') format('embedded-opentype'),
         url('<%= font_path "gt-walsheim-bold-web.woff" %>') format('woff'),
         url('<%= font_path "gt-walsheim-bold-web.ttf" %>') format('truetype'),
         url('<%= font_path "gt-walsheim-bold-web.svg" %>#gt_walsheim_bold') format('svg');
    font-weight: normal;
    font-style: normal;
}

